Listen 8889
<VirtualHost *:8889>
    WSGIDaemonProcess cloudkitty-api processes=4 threads=1 user=cloudkitty group=cloudkitty \
                      display-name=%{GROUP}
    WSGIProcessGroup cloudkitty-api
    {% if ingress_internal and ingress_internal.ingress_path -%}
    WSGIScriptAlias {{ ingress_internal.ingress_path }} {{ wsgi_config.wsgi_public_script }}
    {% endif -%}
    WSGIScriptAlias / {{ wsgi_config.wsgi_public_script }}
    WSGIApplicationGroup %{GLOBAL}
    WSGIPassAuthorization On
    <IfVersion >= 2.4>
      ErrorLogFormat "%{cu}t %M"
    </IfVersion>
    ErrorLog {{ wsgi_config.error_log }}
    CustomLog {{ wsgi_config.custom_log }} combined

    <Directory /usr/bin>
        <IfVersion >= 2.4>
            Require all granted
        </IfVersion>
        <IfVersion < 2.4>
            Order allow,deny
            Allow from all
        </IfVersion>
    </Directory>
</VirtualHost>